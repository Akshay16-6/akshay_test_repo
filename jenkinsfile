pipeline{
    agent any

    stages{
        stage('init'){
            steps{
                git branch: 'master' , url: 'https://github.com/Akshay16-6/cloudbot.war.git'
            }
        }
        stage('build'){
            steps{
                sh "npm i"
                sh "npm run build"
            }
        }
        stage('Test'){

            steps{
                sh "npm test"
                echo "npm successfully build and tested"
            }    
        }
    }
}
